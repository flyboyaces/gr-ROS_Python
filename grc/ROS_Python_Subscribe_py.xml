    <name>Node Name</name>
    <key>node_name</key>
    <type>string</type>
  </param>
  <param>
    <name>Message_Type</name>
    <key>msg_type</key>
    <type>string</type>
  </param>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
